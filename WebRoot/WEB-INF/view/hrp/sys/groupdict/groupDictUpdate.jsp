                <td align="left"></td>
            </tr> 
            <tr>
                <td align="right" class="l-table-edit-td"  style="padding-left:20px;">group_simple：</td>
                <td align="left" class="l-table-edit-td"><input name="group_simple" type="text" id="group_simple" ltype="text"  value="${group_simple}" validate="{required:true,maxlength:20}" /></td>
