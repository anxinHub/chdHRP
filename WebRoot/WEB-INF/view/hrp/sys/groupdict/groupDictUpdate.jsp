                <td align="left" class="l-table-edit-td"><input name="group_id" type="text" id="group_id" ltype="text"  value="${group_id}" validate="{required:true,maxlength:20}" /></td>
                <td align="left"></td>
            </tr> 
            <tr>
                <td align="right" class="l-table-edit-td"  style="padding-left:20px;">group_code：</td>
                <td align="left" class="l-table-edit-td"><input name="group_code" type="text" id="group_code" ltype="text"  value="${group_code}" validate="{required:true,maxlength:20}" /></td>
                <td align="left"></td>
            </tr> 
            <tr>
